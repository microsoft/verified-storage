always-show-logo yes
notify-keyspace-events KEA
daemonize no
pidfile /var/run/redis.pid
port 21342
timeout 0
bind 127.0.0.1
loglevel verbose
logfile ''
databases 16
latency-monitor-threshold 1
save 60 10000
rdbcompression yes
dbfilename dump.rdb
dir ./tests/tmp/server.1600938.1
slave-serve-stale-data yes
appendonly no
appendfsync everysec
no-appendfsync-on-rewrite no
activerehashing yes
pointer-based-aof yes
nvm-maxcapacity 10
nvm-dir /mnt/pmem0/
nvm-threshold 64
maxmemory 1073741824
maxmemory-policy allkeys-lru
