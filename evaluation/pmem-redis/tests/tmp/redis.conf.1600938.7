always-show-logo yes
notify-keyspace-events KEA
daemonize no
pidfile /var/run/redis.pid
port 21344
timeout 0
bind 127.0.0.1
loglevel verbose
logfile ''
databases 16
latency-monitor-threshold 1
save 60 10000
rdbcompression yes
dbfilename hash-zipmap.rdb
dir ./tests/tmp/server.convert-zipmap-hash-on-load.1600938.3
slave-serve-stale-data yes
appendonly no
appendfsync everysec
no-appendfsync-on-rewrite no
activerehashing yes
pointer-based-aof yes
hash-max-ziplist-entries 1
